<Window x:Class="ByteBank.Agencias.MainWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:local="clr-namespace:ByteBank.Agencias" mc:Ignorable="d" Title="MainWindow" Height="380" Width="600">
	<Canvas Name="container">
		<StackPanel Canvas.Top="15" Canvas.Left="320">
			<TextBlock FontWeight="Bold">Number</TextBlock>
			<TextBlock Name="txtNumber"></TextBlock>
			<TextBlock FontWeight="Bold">Name</TextBlock>
			<TextBlock Name="txtName"></TextBlock>
			<TextBlock FontWeight="Bold">Phone</TextBlock>
			<TextBlock Name="txtPhone"></TextBlock>
			<TextBlock FontWeight="Bold">Description</TextBlock>
			<TextBlock Name="txtDescription"></TextBlock>
			<TextBlock FontWeight="Bold">Address</TextBlock>
			<TextBlock Name="txtAddress"></TextBlock>
		</StackPanel>
        <Button Content="Excluir" Canvas.Left="500" Canvas.Top="310" Width="75" Click="Delete" Name="btnDelete"/>
        <Button Content="Editar" Canvas.Left="325" Canvas.Top="310" Width="75" Click="Edit" Name="btnEdit"/>
    </Canvas>
</Window>